<%- include("partials/header") -%>
 <!-- <ul>
    <%# loaClass.forEach(element => { %>
        <li><%#- element %></li>
    <%# }); %>
 </ul> -->

 <div class="main-container">
    <!-- Main Roster Creation -->
    <div>
        <section>
                <div>
                    <form action="/dashboard/createCharacter" method="POST">
                        <label for="characterName">Character Name</label>
                        <input type="text" name="characterName" id="characterName" placeholder="Character Name">
                
                        <label for="loaClass">Class</label>
                        <select name="loaClass" id="loaClass">
                            <% loaClass.forEach(element => { %>
                                <option value="<%- element %>"><%- element %></option>
                            <% }); %>
                        </select>
                
                        <label for="itemLevel">Item Level</label>
                        <input type="text" name="itemLevel" id="itemLevel" placeholder="Item Level">

                        <label for="roster">Roster</label>
                        <select name="roster" id="roster">
                            <option value="Main">Main</option>
                            <option value="Alt">Alt</option>
                        </select>
                
                        <button type="submit">Add</button>
                    </form>
                </div>

                <!-- roster information -->
                    <div class="dashboard">
                        <% for(let i = 0; i < characters.length; i++){ %>
                            <div class="container" id="char<%= i %>">
                                <div class="roster-info">
                                    <h3><%= characters[i].characterName %></h3>
                                    <p><%= characters[i].loaClass %></p>
                                    <p><%= characters[i].itemLevel %></p>
                                </div>

                                <div class="raidList">
                                     <!-- Valtan -->
                                    <div id="raid-select<%= i %>" class="raid-card">
                                        <% if(characters[i].itemLevel >= valtan.normalValtan.itemLevel && valtan.hardValtan.itemLevel){ %>
                                            <div id="valtan" class="card">
                                                <h1>Valtan</h1>
                                                <div class="raid-info more-info">
                                                    <div class="gate1">
                                                        <p><%= valtan.hardValtan.gate[0] %></p>
                                                        <p><%= valtan.hardValtan.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= valtan.hardValtan.gate[1] %></p>
                                                        <p><%= valtan.hardValtan.gold[1] %>g</p>
                                                    </div>
                                                </div>
                                                
                                                <!-- <p><%# valtan.hardValtan.gold.reduce((a,b) => a + b) %>g</p> -->
                                            </div>
                                        <% }else{ %>
                                            <div id="valtan">
                                                <h1>Valtan</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= valtan.normalValtan.gate[0] %></p>
                                                        <p><%= valtan.normalValtan.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= valtan.normalValtan.gate[1] %></p>
                                                        <p><%= valtan.normalValtan.gold[1] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# valtan.normalValtan.gold.reduce((a,b) => a + b) %>g</p> -->
                                            </div>
                                        <% } %>
                                    </div>

                                    <!-- Vykas -->
                                    <div id="raid-select<%= i %>" class="raid-card">
                                        <% if(characters[i].itemLevel >= vykas.normalVykas.itemLevel && vykas.hardVykas.itemLevel){ %>
                                            <div id="vykas" class="card">
                                                <h1>Vykas</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= vykas.hardVykas.gate[0] %></p>
                                                        <p><%= vykas.hardVykas.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= vykas.hardVykas.gate[1] %></p>
                                                        <p><%= vykas.hardVykas.gold[1] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# vykas.hardVykas.gold.reduce((a,b) => a + b) %>g</p> -->
                                            </div>
                                        <% }else{ %>
                                            <div id="vykas" class="card">
                                                <h1>Vykas</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= vykas.normalVykas.gate[0] %></p>
                                                        <p><%= vykas.normalVykas.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= vykas.normalVykas.gate[1] %></p>
                                                        <p><%= vykas.normalVykas.gold[1] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# vykas.normalVykas.gold.reduce((a,b) => a + b) %>g</p> -->
                                            </div>
                                        <% } %>
                                    </div>  

                                    <!-- Kakul Saydon -->
                                    <div id="raid-select<%= i %>" class="raid-card">   
                                        <% if(characters[i].itemLevel >= kakulSaydon.itemLevel){ %>
                                            <div id="kakulSaydon" class="card">
                                                <h1>Kakul-Saydon</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= kakulSaydon.gate[0] %></p>
                                                        <p><%= kakulSaydon.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= kakulSaydon.gate[1] %></p>
                                                        <p><%= kakulSaydon.gold[1] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= kakulSaydon.gate[2] %></p>
                                                        <p><%= kakulSaydon.gold[2] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# kakulSaydon.gold.reduce((a,b) => a + b) %>g</p> -->
                                            </div>
                                        <% } %>
                                    </div>

                                    <!-- Brelshaza -->
                                    <div id="raid-select<%= i %>" class="raid-card">
                                        <% if(characters[i].itemLevel >= brel.hardBrelshaza.itemLevel[2]) { %>
                                            <div id="brelshaza" class="card">
                                                <h1>Brelshaza</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= brel.hardBrelshaza.gate[0] %></p>
                                                        <p><%= brel.hardBrelshaza.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= brel.hardBrelshaza.gate[1] %></p>
                                                        <p><%= brel.hardBrelshaza.gold[1] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= brel.hardBrelshaza.gate[2] %></p>
                                                        <p><%= brel.hardBrelshaza.gold[2] %>g</p>
                                                    </div>

                                                    <div class="gate4">
                                                        <p><%= brel.hardBrelshaza.gate[3] %></p>
                                                        <p><%= brel.hardBrelshaza.gold[3] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# brelGold.hmBrelGold %>g</p> -->

                                            </div>
                                        <% }else if(characters[i].itemLevel >= brel.hardBrelshaza.itemLevel[1] && characters[i].itemLevel < brel.hardBrelshaza.itemLevel[2] ){ %>
                                            <div id="brelshaza" class="card">
                                                <h1>Brelshaza</h1>
                                                
                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= brel.hardBrelshaza.gate[0] %></p>
                                                        <p><%= brel.hardBrelshaza.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= brel.hardBrelshaza.gate[1] %></p>
                                                        <p><%= brel.hardBrelshaza.gold[1] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= brel.hardBrelshaza.gate[2] %></p>
                                                        <p><%= brel.hardBrelshaza.gold[2] %>g</p>
                                                    </div>

                                                    <div class="gate4">
                                                        <p><%= brel.normalBrelshaza.gate[3] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[3] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# brelGold.hm13nm4 %>g</p> -->
                                            </div>
                                        <% }else if(characters[i].itemLevel >= brel.hardBrelshaza.itemLevel[0] && characters[i].itemLevel < brel.hardBrelshaza.itemLevel[1]){ %>
                                            <div id="brelshaza" class="card">
                                                <h1>Brelshaza</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= brel.hardBrelshaza.gate[0] %></p>
                                                        <p><%= brel.hardBrelshaza.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= brel.hardBrelshaza.gate[1] %></p>
                                                        <p><%= brel.hardBrelshaza.gold[1] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= brel.normalBrelshaza.gate[2] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[2] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= brel.normalBrelshaza.gate[3] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[3] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# brelGold.hm12nm34 %>g</p> -->
                                            </div>
                                        <% } else if(characters[i].itemLevel >= brel.normalBrelshaza.itemLevel[2] && characters[i].itemLevel < brel.hardBrelshaza.itemLevel[0]){ %>
                                            <div id="brelshaza" class="card">
                                                <h1>Brelshaza</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= brel.normalBrelshaza.gate[0] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= brel.normalBrelshaza.gate[1] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[1] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= brel.normalBrelshaza.gate[2] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[2] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= brel.normalBrelshaza.gate[3] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[3] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# brelGold.nmBrelGold %>g</p> -->
                                            </div>
                                        <% }else if(characters[i].itemLevel >= brel.normalBrelshaza.itemLevel[1] && characters[i].itemLevel < brel.normalBrelshaza.itemLevel[2]){ %>
                                            <div id="brelshaza" class="card">
                                                <h1>Brelshaza</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= brel.normalBrelshaza.gate[0] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= brel.normalBrelshaza.gate[1] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[1] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= brel.normalBrelshaza.gate[2] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[2] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# brelGold.nm13 %>g</p> -->
                                            </div>
                                        <% } else{ %>
                                            <div id="brelshaza" class="card">
                                                <h1>Brelshaza</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= brel.normalBrelshaza.gate[0] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= brel.normalBrelshaza.gate[1] %></p>
                                                        <p><%= brel.normalBrelshaza.gold[1] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# brelGold.nm12 %>g</p> -->
                                            </div>
                                        <% } %>
                                        
                                    </div>

                                    <!-- Kayangel -->
                                    <div id="raid-select<%= i %>" class="raid-card">
                                        <% if(characters[i].itemLevel >= kayangel.hardKayangel.itemLevel){ %>
                                            <div id="kayangel" class="card">
                                                <h1>Kayangel</h1>
                                                
                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= kayangel.hardKayangel.gate[0] %></p>
                                                        <p><%= kayangel.hardKayangel.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= kayangel.hardKayangel.gate[1] %></p>
                                                        <p><%= kayangel.hardKayangel.gold[1] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= kayangel.hardKayangel.gate[2] %></p>
                                                        <p><%= kayangel.hardKayangel.gold[2] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# kayangel.hardKayangel.gold.reduce((a,b) => a + b) %>g</p> -->
                                            </div>
                                        <% }else if(characters[i].itemLevel < kayangel.hardKayangel.itemLevel && characters[i].itemLevel >= kayangel.normalKayangel.itemLevel){ %>
                                            <div id="kayangel" class="card">
                                                <h1>Kayangel</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= kayangel.normalKayangel.gate[0] %></p>
                                                        <p><%= kayangel.normalKayangel.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= kayangel.normalKayangel.gate[1] %></p>
                                                        <p><%= kayangel.normalKayangel.gold[1] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= kayangel.normalKayangel.gate[2] %></p>
                                                        <p><%= kayangel.normalKayangel.gold[2] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# kayangel.normalKayangel.gold.reduce((a,b) => a + b) %>g</p> -->
                                            </div>
                                        <% } %>
                                    </div>

                                    <!-- Akkan -->
                                    <div id="raid-select<%= i %>" class="raid-card">
                                        <% if(characters[i].itemLevel >= akkan.hardAkkan.itemLevel){ %>
                                            <div id="akkan" class="card">
                                                <h1>Akkan</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= akkan.hardAkkan.gate[0] %></p>
                                                        <p><%= akkan.hardAkkan.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= akkan.hardAkkan.gate[1] %></p>
                                                        <p><%= akkan.hardAkkan.gold[1] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= akkan.hardAkkan.gate[2] %></p>
                                                        <p><%= akkan.hardAkkan.gold[2] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# akkan.hardAkkan.gold.reduce((a,b) => a + b) %>g</p> -->
                                            </div>
                                        <% }else if(characters[i].itemLevel < akkan.hardAkkan.itemLevel && characters[i].itemLevel >= akkan.normalAkkan.itemLevel){ %>
                                            <div id="akkan" class="card">
                                                <h1>Akkan</h1>

                                                <div class="raid-info">
                                                    <div class="gate1">
                                                        <p><%= akkan.normalAkkan.gate[0] %></p>
                                                        <p><%= akkan.normalAkkan.gold[0] %>g</p>
                                                    </div>

                                                    <div class="gate2">
                                                        <p><%= akkan.normalAkkan.gate[1] %></p>
                                                        <p><%= akkan.normalAkkan.gold[1] %>g</p>
                                                    </div>

                                                    <div class="gate3">
                                                        <p><%= akkan.normalAkkan.gate[2] %></p>
                                                        <p><%= akkan.normalAkkan.gold[2] %>g</p>
                                                    </div>
                                                </div>

                                                <!-- <p><%# akkan.normalAkkan.gold.reduce((a,b) => a + b) %>g</p> -->
                                            </div>
                                        <% } %>
                                    </div>

                                </div>
                            </div>
                        <% } %>
                    </div>
        </section>
    </div>

    <!-- Alt Roster Creation -->
</div>

<%- include("partials/footer") -%>